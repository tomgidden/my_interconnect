[Unit]
Description=GidTech Keypad client handler
After=network-online.target autofs.service

[Service]
ExecStart=/root/interconnect/run_keypad.zsh
WorkingDirectory=/root/interconnect
User=root
Group=daemon
SyslogIdentifier=keypad_client
StandardOutput=syslog
Restart=on-failure
KillSignal=SIGINT
#Nice=10

[Install]
WantedBy=multi-user.target
