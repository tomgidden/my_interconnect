[Unit]
Description=GidTech ShuttleXpress client handler
After=network-online.target

[Service]
ExecStart=/root/shuttlexpress/run_client.zsh
WorkingDirectory=/root/shuttlexpress
User=root
Group=daemon
SyslogIdentifier=shuttlexpress_client
StandardOutput=syslog
Restart=on-failure
KillSignal=SIGINT
#Nice=10

[Install]
WantedBy=multi-user.target
